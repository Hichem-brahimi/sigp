{%  extends "base.html.twig" %}
{% block body %}
  
        {% for item in users %}
            <div class="row">
                <div class="col-md-12">
                    <div class="bgc-white bd bdrs-3 p-20 mB-20">
                        <h4 class="c-grey-900 mB-20">Simple Table</h4>
                        <p>Using the most basic table markup, hereâ€™s how <code class="highlighter-rouge">.table</code>-based tables look in Bootstrap. <strong>All table styles are inherited in Bootstrap 4</strong>, meaning any nested tables will be styled in the same manner as the parent.</p>
                        <table class="table">
                            <thead>
                            <tr>
                                <th scope="col">prenom</th>
                                <th scope="col">nom</th>
                            </tr>
                            </thead>
                            <tbody>
                            <tr>
                                <th scope="row">1</th>
                                <td>{{ item.Nom }}</td>
                                <td>{{ item.Prenom }}</td>
                            </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

            <div class="bgc-white p-20 bd">
                <h6 class="c-grey-900">Alerts</h6>
                <div class="mT-30">
                    <div class="alert alert-primary" role="alert">
                        {{ item.Nom }} | {{ item.Prenom }}
                    </div>
                </div>
            </div>
        {% endfor %}
{% endblock body %}
